{
	"comment": "similar to Section 2.5 in http://www.csrc.sdsu.edu/research_reports/CSRCR2012-01.pdf",
	"output-directory": "results/hydrodynamic/jet_injection/roe_athena",
	"time-start": 0,
	"time-length": 2,
	"minimum-pressure": 0,
	"resistivity": "0",
	"solver-mhd": "roe-athena",
	"load-balancer": "RANDOM",
	"save-mhd-n": 0.1,
	"adiabatic-index": 1.666666667,
	"vacuum-permeability": 1,
	"proton-mass": 1,
	"remove-div-B-n": -1,
	"time-step-factor": 0.5,
	"poisson-norm-stop": 1e-10,
	"poisson-norm-increase-max": 10,
	"grid-options": {
		"periodic": "{false, false, false}",
		"cells": "{40 + 2, 20 + 2, 1}",
		"volume": "{3 * (1 + 2 / (cells[0] - 2)), 1.5 * (1 + 2 / (cells[1] - 2)), 1}",
		"start": "{-volume[0] / cells[0], -volume[1] / cells[1], 0}"
	},
	"geometries": [
		{"box": {
			"comment": "-x edge",
			"start": [-99, -99, -99],
			"end": [-0.0001, 99, 99]
		}},
		{"box": {
			"comment": "injection",
			"start": [1.4001, -99, -99],
			"end": [1.5999, -0.0001, 99]
		}},
		{"box": {
			"comment": "+x edge",
			"start": [3.0001, -99, -99],
			"end": [99, 99, 99]
		}},
		{"box": {
			"comment": "+y edge",
			"start": [-99, 1.5001, -99],
			"end": [99, 99, 99]
		}},
		{"box": {
			"comment": "-x side of -y edge",
			"start": [0.0001, -99, -99],
			"end": [1.3999, -0.0001, 99]
		}},
		{"box": {
			"comment": "+x side of -y edge",
			"start": [1.6001, -99, -99],
			"end": [99, -0.0001, 99]
		}}
	],
	"number-density": {
		"default": 1,
		"value-boundaries": [
			{
				"comment": "inflow",
				"geometry-id": 0,
				"time-stamps": [0],
				"values": [1]
			},
			{
				"comment": "injection",
				"geometry-id": 1,
				"time-stamps": [0],
				"values": [5]
			}
		],
		"copy-boundaries": [
			{"geometry-id": 2},
			{"geometry-id": 3},
			{"geometry-id": 4},
			{"geometry-id": 5}
		]
	},
	"velocity": {
		"default": "{2.7 - x/2, 0, 0}",
		"value-boundaries": [
			{"geometry-id": 0, "time-stamps": [0], "values": [[2.7, 0, 0]]},
			{"geometry-id": 1, "time-stamps": [0], "values": [[0, 1.3, 0]]}
		],
		"copy-boundaries": [
			{"geometry-id": 2},
			{"geometry-id": 3},
			{"geometry-id": 4},
			{"geometry-id": 5}
		]
	},
	"pressure": {
		"default": 1,
		"value-boundaries": [
			{"geometry-id": 0, "time-stamps": [0], "values": [1]},
			{"geometry-id": 1, "time-stamps": [0], "values": [5]}
		],
		"copy-boundaries": [
			{"geometry-id": 2},
			{"geometry-id": 3},
			{"geometry-id": 4},
			{"geometry-id": 5}
		]
	},
	"magnetic-field": {
		"default": [0, 0, 0],
		"comment": "Following required so that corner cells will be classified dont_solve",
		"value-boundaries": [
			{"geometry-id": 0, "time-stamps": [0], "values": [[0, 0, 0]]},
			{"geometry-id": 1, "time-stamps": [0], "values": [[0, 0, 0]]}
		],
		"copy-boundaries": [
			{"geometry-id": 2},
			{"geometry-id": 3},
			{"geometry-id": 4},
			{"geometry-id": 5}
		]
	}
}
